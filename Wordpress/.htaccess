# BEGIN WordPress
<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /

# BEGIN CakePress

# Use the /webapp Alias for serving asset files
RewriteRule     ^app\/webroot\/(.*)$     /webapp/$1                                [QSA,L]

# End CakePress

RewriteRule ^index\.php$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /index.php [L]
</IfModule>
# END WordPress
